      <script
  src="https://code.jquery.com/jquery-2.2.4.js"
  integrity="sha256-iT6Q9iMJYuQiMWNd9lDyBUStIq/8PuOW33aOqmvFpqI="
  crossorigin="anonymous"></script>
        <script type="text/javascript">

                  $(document).ready(function() {

                    var frm = $('#phancongform');
                    var bodytag=$('body');
                    var urlimage='/images/loadgif.gif';
                     var message=$('#message');
                     var content1=$('#content1');
                     
                     content1.hide();        

                    frm.submit(function(e){


                      var data={};
                     
                       var val = $('#input1').val()
                        var xyz1 = $('#listtaixe option').filter(function() {
                            return this.value == val;
                        }).data('xyz');

                        data.mataixe=xyz1;
                        


                                     
                      var val = $('#input2').val()
                        var xyz2 = $('#listmaxe option').filter(function() {
                            return this.value == val;
                        }).data('xyz');
                         data.maxe=xyz2;


                        data.ngaybatdau=$('#single_cal1').val();
                       data.ngayketthuc=$('#single_cal2').val();



                       e.preventDefault(); // avoid to execute the actual submit of the form.
    
                       //hiệu ứng asyn


 
                      $('body').append('<div id="requestOverlay" class="request-overlay"></div>');
                      $("#requestOverlay").show();/*Show overlay*/

                          $.ajax({

                              type:'POST',
                              url:'/admin/phancong-taixe/phutrachxe/submit',
                              data:JSON.stringify(data),
                              dataType: 'json', 
                              async: true,
                              contentType: 'application/json; charset=utf-8',

                            
                           
                              success:function(result)
                              {
                                

                                if(result.error != null )
                                {
                                   message.empty();
                                  message.append(result.error)                                                                             

                                }

                                else{
                                  
                                  content1.empty();
                                  content1.append('<div class=""><div class="page-title"><div class="title_left"><h3>THÔNG BÁO</h3></div></div><div class="clearfix"></div><div class="row"><div class="alert alert-success">'+result.message+'</div></div></div>');        


                              
                              }
                            },
                              error:function(err)
                              {
                              

                                console.log(err);
                              },
                              complete:function()
                              {
                                 $("#requestOverlay").remove();/*Remove overlay*/
                              }
                          });                 
                        
                    });

                   
                  });
          </script>



