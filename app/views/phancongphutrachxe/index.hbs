                         <input  list="listmaxe" id="input2" name="listmaxe">
                            <datalist id="listmaxe" >
                              {{#each ListXe}}
                              <option value="{{this.MaXe}}" data-xyz="{{this.MaXe}}">
                             
                              {{/each}}
                            </datalist>
                        

                        </div>
                    
                      </div>


                       <div class="item form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="occupation">Ngày Bắt Đầu <span class="required" style="color:red">*</span>
                        </label>
                     
                          <fieldset>
                          <div class="control-group">
                            <div class="controls">
                              <div class="col-md-5 xdisplay_inputx form-group has-feedback">
                                <input type="text" class="form-control has-feedback-left" id="single_cal1" name="ngaysinh" value={{CurrentNhanVien.NgaySinhNV}} aria-describedby="inputSuccess2Status">
                                <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
                                <span id="inputSuccess2Status" class="sr-only">(success)</span>
