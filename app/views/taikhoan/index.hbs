 <div class="right_col" role="main">
          <div class="">
            

            <div class="clearfix"></div>

            <div class="row">
              <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2><i>DANH SÁCH TÀI KHOẢN KHÁCH HÀNG</i></h2>
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                      </li>
                      <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                        <ul class="dropdown-menu" role="menu">
                          <li><a href="#">Settings 1</a>
                          </li>
                          <li><a href="#">Settings 2</a>
                          </li>
                        </ul>
                      </li>
                      <li><a class="close-link"><i class="fa fa-close"></i></a>
                      </li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">
                  
                    <p class="text-muted font-13 m-b-30">
                     
                    </p>

                    <form id="searchtkkh">
                     
                      
                    
                            
                              <div class="input-group">
                                <input type="text" id="tbltk" placeholder="Nhập Loại Tài Khoản..." class="form-control">
                                <span class="input-group-btn">
                                  <input type="submit" id="searchltk" class="btn btn-primary" value="Search">
                                  </span>
                              </div>
                      

                    </form>
                  
                  
                 
                 <!-- tim nhan vien by CMND -->
                
                        


                    <table id="datatable" class="table table-striped table-bordered">
                      <thead>
                        <tr>
                          <th>Mã TK</th>
                          <th>Điểm</th>
                          <th>Loại TK</th>
                          <th>Số Tiền</th>                       
                          <th>Tên Đăng Nhập</th>                      
                          <th>Mã Khách Hàng</th>
                           <th>Action</th>
                        </tr>
                      </thead>


                      <tbody>
                        {{#each ListTaiKhoan}}
                        <tr>
                          <td>{{this.MaTaiKhoan}}</td>
                          <td>{{this.DiemTichLuy}}</td>
                          <td>{{this.LoaiTaiKhoan}}</td>
                          <td>{{this.SoTien}}</td>
                          <td>{{this.TenDangNhap}}</td>
                          
                          <td>{{this.MaKhachHang}}</td>
                          <td>
                           <!--  <a href="#" class="btn btn-success">Xem</a> -->
                            <a href="/admin/listtaikhoan/update/mtk={{this.MaTaiKhoan}}" class="btn btn-warning">Cập Nhật</a>

                          
                            
                          </td>
                          
                        </tr>
                        {{/each}}
                       
                      </tbody>
                    </table>
                  </div>
                 
                </div>
                <p class="notify" style="color: red;font-size: 18px;"></p>

              </div>

              </div>

              </div>
              </div>
       <script
  src="https://code.jquery.com/jquery-2.2.4.js"
  integrity="sha256-iT6Q9iMJYuQiMWNd9lDyBUStIq/8PuOW33aOqmvFpqI="
  crossorigin="anonymous"></script>
        <script type="text/javascript">

                  $(document).ready(function() {

                    var frm = $('#searchtkkh');
                    var bodytag=$('body');
                    var urlimage='/images/loadgif.gif';


               

                    frm.submit(function(e){

                       e.preventDefault(); // avoid to execute the actual submit of the form.
    
                       //hiệu ứng asyn

 
                      $('body').append('<div id="requestOverlay" class="request-overlay"></div>');
                      $("#requestOverlay").show();/*Show overlay*/

                          $.ajax({

                              type:'GET',
                              url:'/admin/listtaikhoan/findtk?ltk='+$('#tbltk').val(),
                              
                              dataType: 'json', 
                              async: true,
                              contentType: 'application/json; charset=utf-8',

                            
                           
                              success:function(result)
                              {
                               
                               var tbodyobj=$('tbody');
                               var notify=$('.notify');


                                if(result.recordset.length == 0)
                                {
                                 notify.empty();
                                notify.append('Không tìm thấy tài khoản nào');

                                  tbodyobj.empty();                                                        

                                }

                                else{
                                  notify.empty();

                                  tbodyobj.empty();
                                  
                              for(var i=0;i<result.recordset.length;i++)
                              {
                                 tbodyobj.append('\
                                    <tr>\
                          <td>'+result.recordset[i].MaTaiKhoan+'</td>\
                          <td>'+result.recordset[i].DiemTichLuy+'</td>\
                          <td>'+result.recordset[i].LoaiTaiKhoan+'</td>\
                          <td>'+result.recordset[i].SoTien+'</td>\
                          <td>'+result.recordset[i].TenDangNhap+'</td>\
                          <td>'+result.recordset[i].MaKhachHang+'</td>\
                          <td>\
                            <a href="#" class="btn btn-danger">Xóa</a>\
                            <a href="/admin/listtaikhoan/update/mtk='+result.recordset[i].MaTaiKhoan+'" class="btn btn-warning">Sửa</a>\
                          </td>\
                        </tr>\
                                    ');
                                }

                                }
                              
                              },
                              error:function(err)
                              {
                                console.log(err);
                              },
                              complete:function()
                              {
                                 $("#requestOverlay").remove();/*Remove overlay*/
                              }
                          });                 
                        
                    });

                   
                  });
          </script>

