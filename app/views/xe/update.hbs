                    <div class="col-md-9 col-sm-9 col-xs-12"> 
                    <div id="collapseOne1" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                    <form action="/admin/listxe/update/submit" method="POST" class="form-horizontal form-label-left" novalidate>
                    
                   
                     
                      <span class="section">Cập Nhật Xe</span>
                      <div class="item form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="name">Mã Xe <span class="required" style="color:red">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          <input id="maxe" readonly class="form-control col-md-7 col-xs-12"  value="{{CurrentXe.MaXe}}"  name="maxe"  required type="text">
                        </div>
                      </div>

                      <div class="item form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="name">Loại Xe <span class="required" style="color:red">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          <input id="loaixe" class="form-control col-md-7 col-xs-12" data-validate-length-range="6" value="{{CurrentXe.LoaiXe}}" data-validate-words="2" name="loaixe"  required type="text">
                        </div>
                      </div>
                      
                      
                       <div class="item form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="name">Số Lượng Ghế <span class="required" style="color:red">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          <input id="slg" class="form-control col-md-7 col-xs-12" data-validate-length-range="6" value="{{CurrentXe.SoLuongGhe}}" data-validate-words="2" name="slg"  required type="text">
                        </div>
                      </div>
                       

                       <div class="item form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="name">Biển Số Xe <span class="required" style="color:red">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          <input id="bsx" class="form-control col-md-7 col-xs-12" data-validate-length-range="6" value="{{CurrentXe.BienSoXe}}" data-validate-words="2" name="bsx"  required type="text">
                        </div>
                      </div>

                      
                     
                      <div class="ln_solid"></div>
                      <div class="form-group">
                        <div class="col-md-6 col-md-offset-3">
                         
                          <button id="send" type="submit" class="btn btn-success">Update</button>
                        </div>
                      </div>
